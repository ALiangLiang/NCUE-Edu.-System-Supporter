{
	"background": {
		"scripts": [  "background.js" ]
	},
	"content_scripts": [ {
		"css": [  "Bootstrap/css/bootstrap.min.css" ],
		"js": [  "jquery/jquery-2.1.0.min.js", "Bootstrap/js/bootstrap.min.js",  "main.js" ],
		"matches": [ 
			"*://*.ncue.edu.tw/*/", 
			"*://*.ncue.edu.tw/*/*.aspx"
		]
	}, {
		"all_frames": true,
		"js": [  "start.js" ],
		"matches": [ 
			"*://webap.ncue.edu.tw/*/", 
			"*://webap.ncue.edu.tw/*/*.aspx",  
			"*://webap.ncue.edu.tw/other/ob/OB010.aspx*",
			"*://webap.ncue.edu.tw/information/News/left_news.*",
			"*://webap0.ncue.edu.tw/*/", 
			"*://webap0.ncue.edu.tw/*/*.aspx",  
			"*://webap0.ncue.edu.tw/other/ob/OB010.aspx*",
			"*://webap0.ncue.edu.tw/information/News/left_news.*"
		],
		"run_at": "document_start"
	}, {
		"all_frames": true,
		"js": [ "subAfter.js" ],
		"matches": [ "*://*.ncue.edu.tw/information/News/left_news.*" ]
	}, {
		"all_frames": true,
		"js": [ "supporter.js" ],
		"matches": [ "*://webap.ncue.edu.tw/*", "*://webap0.ncue.edu.tw/*" ],
		"run_at": "document_start"
	}, {
		"all_frames": true,
		"js": [ "supporter_after.js" ],
		"matches": [ "*://*.ncue.edu.tw/STU/C/SC020.aspx?*" ],
		"run_at": "document_end"
	}, {
		"all_frames": true,
		"js": [ "supporter_after_tree.js" ],
		"matches": [ "*://*.ncue.edu.tw/ST/SYS/Frame_Menu.aspx" ],
		"run_at": "document_end"
	} ],
	"content_security_policy": "script-src 'self' https://www.google.com https://checkout.google.com; object-src 'self'",
	"description": "破解限用IE瀏覽器，自動填入驗證碼",
	"icons": {
		"128": "icon/icon128.png",
		"16": "icon/icon16.png",
		"48": "icon/icon48.png"
	},
	"key": "",
	"manifest_version": 2,
	"name": "NCUE新教務系統 Supporter",
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.3.0",
	"permissions": [
		"tabs", "http://*/*"
	]
}
